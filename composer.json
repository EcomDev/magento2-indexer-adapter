{
    "name": "ecomdev/indexer-wrapper",
    "description": "Database Wrapper for Magento 2",
    "type": "magento2-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Chepurnyi",
            "email": "ivan.chepurnyi@ecomdev.org"
        }
    ],
    "minimum-stability": "stable",
    "require": {
    },
    "autoload": {
        "psr-4": {
            "EcomDev\\DatabaseWrapper\\": "src/library"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "EcomDev\\DatabaseWrapper\\": "tests/library"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "require-dev": {
        "magento/framework": "^101.0",
        "magento/zendframework1": "^1.13",
        "phpunit/phpunit": "^6.4",
        "phpunit/dbunit": "^3.0",
        "squizlabs/php_codesniffer": "^3.1",
        "phpmd/phpmd": "^2.6",
        "sebastian/comparator": "^2.1"
    }
}
